<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--#region Gradient Color-->
   

    <LinearGradientBrush x:Key="cs_PopupBackground" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#ff2b84ac" Offset="0.0"/>
        <GradientStop Color="#ff97d5f2" Offset="1.2"/>
    </LinearGradientBrush>

    <!--#endregion-->

    <!--#region solid color-->
    <SolidColorBrush x:Key="cs_Menu" Color="#FFE3B477"/>
    <SolidColorBrush x:Key="cs_Section" Color="#FFF28F6A"/>

    <SolidColorBrush x:Key="cs_Background" Color="#FFF2CB6A"/>

    <SolidColorBrush x:Key="cs_BackgroundLayer" Color="#FFFFFFBA" Opacity="1"/>
    <SolidColorBrush x:Key="cs_BorderBrush" Color="DarkSlateGray"/>
    

    <!--#endregion ff97ceda -->
</ResourceDictionary>